<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
"http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.ruoyi.system.tile.mapper.TileStockInMapper">
    
    <resultMap type="TileStockIn" id="TileStockInResult">
        <result property="inId"        column="in_id"        />
        <result property="inCode"      column="in_code"      />
        <result property="warehouseId" column="warehouse_id" />
        <result property="supplierId"  column="supplier_id"  />
        <result property="inType"      column="in_type"      />
        <result property="status"      column="status"       />
        <result property="totalAmount" column="total_amount" />
        <result property="createBy"    column="create_by"    />
        <result property="createTime"  column="create_time"  />
        <result property="updateBy"    column="update_by"    />
        <result property="updateTime"  column="update_time"  />
        <result property="remark"      column="remark"       />
    </resultMap>

    <sql id="selectTileStockInVo">
        select in_id, in_code, warehouse_id, supplier_id, in_type, status, total_amount, create_by, create_time, update_by, update_time, remark
        from tile_stock_in
    </sql>

    <select id="selectTileStockInList" parameterType="TileStockIn" resultMap="TileStockInResult">
        <include refid="selectTileStockInVo"/>
        <where>  
            <if test="inCode != null  and inCode != ''"> and in_code = #{inCode}</if>
            <if test="warehouseId != null "> and warehouse_id = #{warehouseId}</if>
            <if test="supplierId != null "> and supplier_id = #{supplierId}</if>
            <if test="inType != null  and inType != ''"> and in_type = #{inType}</if>
            <if test="status != null  and status != ''"> and status = #{status}</if>
        </where>
        order by create_time desc
    </select>
    
    <select id="selectTileStockInByInId" parameterType="Long" resultMap="TileStockInResult">
        <include refid="selectTileStockInVo"/>
        where in_id = #{inId}
    </select>
        
    <insert id="insertTileStockIn" parameterType="TileStockIn" useGeneratedKeys="true" keyProperty="inId">
        insert into tile_stock_in
        <trim prefix="(" suffix=")" suffixOverrides=",">
            <if test="inCode != null">in_code,</if>
            <if test="warehouseId != null">warehouse_id,</if>
            <if test="supplierId != null">supplier_id,</if>
            <if test="inType != null">in_type,</if>
            <if test="status != null">status,</if>
            <if test="totalAmount != null">total_amount,</if>
            <if test="createBy != null">create_by,</if>
            <if test="createTime != null">create_time,</if>
            <if test="updateBy != null">update_by,</if>
            <if test="updateTime != null">update_time,</if>
            <if test="remark != null">remark,</if>
         </trim>
        <trim prefix="values (" suffix=")" suffixOverrides=",">
            <if test="inCode != null">#{inCode},</if>
            <if test="warehouseId != null">#{warehouseId},</if>
            <if test="supplierId != null">#{supplierId},</if>
            <if test="inType != null">#{inType},</if>
            <if test="status != null">#{status},</if>
            <if test="totalAmount != null">#{totalAmount},</if>
            <if test="createBy != null">#{createBy},</if>
            <if test="createTime != null">#{createTime},</if>
            <if test="updateBy != null">#{updateBy},</if>
            <if test="updateTime != null">#{updateTime},</if>
            <if test="remark != null">#{remark},</if>
         </trim>
    </insert>

    <update id="updateTileStockIn" parameterType="TileStockIn">
        update tile_stock_in
        <trim prefix="SET" suffixOverrides=",">
            <if test="inCode != null">in_code = #{inCode},</if>
            <if test="warehouseId != null">warehouse_id = #{warehouseId},</if>
            <if test="supplierId != null">supplier_id = #{supplierId},</if>
            <if test="inType != null">in_type = #{inType},</if>
            <if test="status != null">status = #{status},</if>
            <if test="totalAmount != null">total_amount = #{totalAmount},</if>
            <if test="updateBy != null">update_by = #{updateBy},</if>
            <if test="updateTime != null">update_time = #{updateTime},</if>
            <if test="remark != null">remark = #{remark},</if>
        </trim>
        where in_id = #{inId}
    </update>

    <delete id="deleteTileStockInByInId" parameterType="Long">
        delete from tile_stock_in where in_id = #{inId}
    </delete>

    <delete id="deleteTileStockInByInIds" parameterType="String">
        delete from tile_stock_in where in_id in 
        <foreach item="inId" collection="array" open="(" separator="," close=")">
            #{inId}
        </foreach>
    </delete>
</mapper>
